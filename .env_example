# LLM-SPT .env 模板（由 --init-config 生成）
# 优先级：CLI > ENV(.env) > JSON
# 空值表示未设置；按需填写后移除本行注释。

# 配置来源（可二选一）
LLM_SPT_CONFIG_FILE=
LLM_SPT_CONFIG_JSON=

# 运行参数覆盖
LLM_SPT_INPUTS=
LLM_SPT_CONCURRENCY=
LLM_SPT_MAX_TOKENS=
LLM_SPT_MAX_RETRIES=
LLM_SPT_LLM=

# 组件选择
LLM_SPT_COMPONENTS_READER=
LLM_SPT_COMPONENTS_SPLITTER=
LLM_SPT_COMPONENTS_BATCHER=
LLM_SPT_COMPONENTS_WRITER=
LLM_SPT_COMPONENTS_PROMPT_BUILDER=
LLM_SPT_COMPONENTS_DECODER=
LLM_SPT_COMPONENTS_ASSEMBLER=

# Provider 覆盖（openai）
LLM_SPT_PROVIDER__openai__CLIENT=
LLM_SPT_PROVIDER__openai__LIMITS_RPM=
LLM_SPT_PROVIDER__openai__LIMITS_TPM=
LLM_SPT_PROVIDER__openai__LIMITS_MAX_TOKENS_PER_REQ=
LLM_SPT_PROVIDER__openai__OPTIONS_JSON=

# Provider 覆盖（gemini）
LLM_SPT_PROVIDER__gemini__CLIENT=
LLM_SPT_PROVIDER__gemini__LIMITS_RPM=
LLM_SPT_PROVIDER__gemini__LIMITS_TPM=
LLM_SPT_PROVIDER__gemini__LIMITS_MAX_TOKENS_PER_REQ=
LLM_SPT_PROVIDER__gemini__OPTIONS_JSON=

# 常见供应商 API Key
OPENAI_API_KEY=
GOOGLE_API_KEY=

